<?xml version="1.0" encoding="UTF-8" ?>
<interface namespace="Core" name="Router">
	<internal include="conct_router.h" class="Router" />
	<singleton />
	<properties>
		<property name="ConnectedDevices">
			<type>
				<array type="DeviceConnection" />
			</type>
			<get />
		</property>
	</properties>
	<functions>
		<function name="changeDevice" return="Void">
			<parameters>
				<parameter name="deviceId" type="DeviceId" />
				<parameter name="deviceStatus" type="DeviceStatus" />
			</parameters>
		</function>
	</functions>
</interface>